<Window x:Class="MemoryV._5.OptionsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="OptionsWindow" Height="300" Width="300">
    <Grid>
        <StackPanel Margin="5" Orientation="Horizontal">
            <Label>Icon Set:</Label>
            <ComboBox Width="200" 
                      ItemsSource="{Binding Path=AvailibleIconSets}"
                      SelectedItem="{Binding Path=SelectedIconSet}" Margin="0,0,0,220" 
                />
            
        </StackPanel>
        
        
    </Grid>
</Window>
